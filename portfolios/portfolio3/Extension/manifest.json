{
  "manifest_version": 2,

  "name": "myDash",
  "description": "Portfolio 3 for Com S 319",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
    "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  
  "permissions": [
    "activeTab",
	"tabs",
    "https://ajax.googleapis.com/"
  ],
  
  "content_security_policy": "script-src 'self'; object-src 'self'"
}